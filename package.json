{
  "name": "popcorn-time-desktop",
  "version": "0.2.0",
  "description": "An experimental Popcorn Time client",
  "keywords": [
    "client",
    "electron",
    "es6",
    "popcorn",
    "popcorn-time",
    "react",
    "redux",
    "time"
  ],
  "homepage": "https://github.com/tripss/popcorn-time-desktop",
  "bugs": {
    "url": "https://github.com/tripss/popcorn-time-desktop/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Tycho Bokdam",
    "email": "t.bokdam@seronaris.nl",
    "url": "https://github.com/tripss"
  },
  "directories": {
    "buildResources": "resources",
    "output": "release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tripss/popcorn-time-desktop.git"
  },
  "scripts": {
    "bench-api": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 jest test/**/*.benchmark.js",
    "build": "cross-env NODE_ENV=production npm run build-main && npm run build-renderer",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config ./build/webpack.config.main.prod.js --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config ./build/webpack.config.renderer.prod.js --colors",
    "build:dll": "node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config ./build/webpack.config.renderer.dll.js --colors",
    "build:dll:dev": "cross-env NODE_ENV=development npm run build:dll",
    "build:dll:prod": "cross-env NODE_ENV=production npm run build:dll",
    "electron-rebuild": "electron-rebuild --version=1.7.0 --force --types prod,dev,optional && electron-rebuild --version=1.7.0 --force --types prod,dev,optional --module-dir app",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "postinstall": "npm run build:dll:dev && node node_modules/fbjs-scripts/node/check-dev-engines.js package.json && node -r babel-register postinstall.js",
    "lint:check": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --format=node_modules/eslint-formatter-pretty app *.js",
    "lint:js:fix": "eslint --fix --format=node_modules/eslint-formatter-pretty app *.js",
    "lint:style": "stylelint 'app/**/*.scss' --syntax scss --color",
    "lint:style:fix": "stylefmt -r app/**/*.scss",
    "lint:summary": "eslint-nibble app *.js",
    "package": "npm run postinstall && npm run build && build --publish never",
    "package:ci": "npm run postinstall && npm run build && build --publish onTagOrDraft",
    "package:all": "npm run postinstall && npm run build && build --publish never --mac --win --linux",
    "package:mac": "cross-env NODE_ENV=production npm run build:dll && npm run build && build --publish never --mac",
    "package:mac:debug": "cross-env DEBUG_PROD=true npm run package:mac",
    "package:win": "cross-env NODE_ENV=production npm run postinstall && npm run build && build --publish never --win",
    "package:win:debug": "cross-env DEBUG_PROD=true npm run package:win",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./app/main.dev",
    "start-renderer-dev": "cross-env NODE_ENV=development HOT=1 node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./build/webpack.config.renderer.dev.js",
    "start:dev": "cross-env DEBUG=app:*,api:* NODE_ENV=development START_HOT=1 npm run start-renderer-dev",
    "start:prod": "cross-env DEBUG=app:*,api:* NODE_ENV=production START_HOT=1 npm run start-renderer-dev",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 jest ./test/components/*.spec.js",
    "test-all": "npm run lint && npm run lint-styles && npm run lint-summary && npm run build && npm run test-api && npm run test-e2e",
    "test-api": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 CONFIG_ENDPOINT_TEST=test.org jest ./test/api/butter.spec.js",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 jest ./test/e2e.spec.js",
    "test-screenshot": "cross-env API_USE_MOCK_DATA=true BABEL_DISABLE_CACHE=1 npm run build && jest ./test/screenshot.e2e.js",
    "test-watch": "npm test -- --watch",
    "prettier": "prettier --single-quote --write \"./{app,test}/{actions,api,components,containers,reducers,utils}/**/*.js\" && prettier --single-quote --write \"./app/{actions,api,components,containers,reducers,utils}/**/*.jsx\""
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "axios": "^0.16.2",
    "bootstrap": "4.0.0-alpha.4",
    "debug": "^2.6.8",
    "has-own-property": "^1.0.0",
    "image-webpack-loader": "^3.3.1",
    "isomorphic-fetch": "2.2.1",
    "nedb": "^1.8.0",
    "network-address": "1.1.2",
    "opensubtitles-api": "4.0.0",
    "plyr": "2.0.13",
    "portfinder": "^1.0.13",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-addons-transition-group": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-hot-loader": "3.0.0-beta.7",
    "react-redux": "5.0.5",
    "react-router": "4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-star-rating-component": "1.3.0",
    "react-toggle-switch": "^2.1.4",
    "react-tooltip": "^3.3.0",
    "react-transition-group": "^2.2.0",
    "react-visibility-sensor": "3.10.1",
    "reactstrap": "^4.8.0",
    "redux": "^3.7.2",
    "redux-clazz": "^2.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "rndm": "1.2.0",
    "semver": "5.4.1",
    "speedtest-net": "1.3.1",
    "srt2vtt": "1.3.1",
    "super-kat": "0.1.0",
    "torrentapi-wrapper": "0.0.2",
    "trakt.tv": "github:tripss/trakt.tv",
    "vlc-command": "1.1.1",
    "webtorrent": "^0.98.19"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-dynamic-import-webpack": "1.0.1",
    "babel-plugin-flow-runtime": "0.11.1",
    "babel-plugin-module-alias": "1.6.0",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-remove-console": "6.8.4",
    "babel-plugin-transform-remove-debugger": "6.8.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-webpack-loaders": "0.9.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "6.25.0",
    "chai": "^4.1.0",
    "chalk": "^2.0.1",
    "classnames": "2.2.5",
    "concurrently": "3.5.0",
    "configstore": "3.1.1",
    "cross-env": "^5.0.3",
    "css-loader": "0.28.4",
    "css-modules-require-hook": "4.0.6",
    "devtron": "1.4.0",
    "download": "^6.2.5",
    "electron": "^1.6.11",
    "electron-builder": "^19.19.1",
    "electron-debug": "^1.3.0",
    "electron-devtools-installer": "2.2.0",
    "electron-rebuild": "^1.6.0",
    "electron-squirrel-startup": "1.0.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "1.9.0",
    "eslint-nibble-ignore": "3.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "20.0.3",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "4.15.3",
    "extract-text-webpack-plugin": "3.0.0",
    "extract-zip": "1.6.5",
    "fbjs-scripts": "0.8.1",
    "file-loader": "0.11.2",
    "flow-bin": "^0.52.0",
    "flow-runtime": "^0.14.0",
    "flow-typed": "2.1.5",
    "gm": "1.23.0",
    "image-diff": "1.6.3",
    "immutable": "3.8.1",
    "jest-cli": "^20.0.4",
    "jsdom": "11.1.0",
    "json-loader": "0.5.7",
    "node-libs-browser": "2.0.0",
    "node-loader": "0.6.0",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.6",
    "prettier": "^1.5.3",
    "sass-loader": "6.0.6",
    "sinon": "3.0.0",
    "source-map-support": "0.4.15",
    "spectron": "3.7.2",
    "style-loader": "0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "17.0.0",
    "uglifyjs-webpack-plugin": "1.0.0-beta.1",
    "url-loader": "0.5.9",
    "webpack": "3.4.1",
    "webpack-bundle-analyzer": "2.8.3",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "2.6.1",
    "webpack-hot-middleware": "2.18.2",
    "webpack-merge": "4.1.0",
    "why-did-you-update": "0.0.8"
  },
  "build": {
    "appId": "com.github.popcorn-time-desktop",
    "productName": "Popcorn Time",
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "dmg": {
      "format": "ULFO",
      "icon": "./resources/icon.icns",
      "window": {
        "width": "500",
        "height": "300"
      },
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "./resources/icon.ico"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "devEngines": {
    "node": ">=8.x",
    "npm": ">=4.x"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    }
  }
}
